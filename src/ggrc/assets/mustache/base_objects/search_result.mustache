{{#list}}
<li class="{{constructor.category}}" data-model="true" {{data 'model'}}>
  <a href="{{viewLink}}" class="show-extended">
    <div class="row-fluid">
      <div class="span8 lhs-main-title">
        <span class="lhs-item">
          {{title}}
        </span>
      </div>
      <div class="span1 lhs-private">
        {{#is_private}}
          <i class="grcicon-private" rel="tooltip" title="Private"></i>
        {{/is_private}}
      </div>
      <div class="span1 lhs-people">
      {{^owner}}
        <i class="grcicon-people-warning"></i>
      {{/owner}}
      </div>
    </div>
  </a>
</li>
{{/list}}
{{^list}}
<li class="spinner">
</li>
{{/list}}
{{^is_allowed 'read' model.shortName context=null}}
<li class="no-access">
  You do not have access to read this information.
</li>
{{/is_allowed}}
{{^if_equals list.length count}}
<li class="view-more">
  <a href="javascript://">
    view more
    <i class="grcicon-down"></i>
  </a>
</li>
{{/if_equals}}
{{#is_allowed 'create' model.shortName}}
<li class="add-new">
  <a data-object-plural="{{model.table_plural}}" data-modal-class="modal-wide" href="javascript://" data-object-singular="{{model.model_singular}}" data-toggle="modal-ajax-form" data-modal-reset="reset" >
    + Create New
  </a>
</li>
{{/is_allowed}}
